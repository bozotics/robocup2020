{
    "env": {
        "Arduino_Core_STM32_version": "1.9.0"
    },
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceRoot}",
                "~/Documents/Arduino/libraries/**",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/libraries/**",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/cores/arduino",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/cores/arduino/stm32",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/cores/arduino/stm32/LL",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/variants/MAPLEMINI_F103CB",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/system/STM32F1xx",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/system/Drivers/STM32F1xx_HAL_Driver/Src",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/system/Drivers/STM32F1xx_HAL_Driver/Inc",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/system/Drivers/CMSIS/Device/ST/STM32F1xx/Include",
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/system/Drivers/CMSIS/Device/ST/STM32F1xx/Source/Templates",
                "~/Library/Arduino15/packages/STM32/tools/**"
            ],
            "forcedInclude": [
                "~/Library/Arduino15/packages/STM32/hardware/stm32/${Arduino_Core_STM32_version}/cores/arduino/Arduino.h",
                "common.h"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "~/Library/Arduino15/packages/STM32/tools/xpack-arm-none-eabi-gcc/9.2.1-1.1/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "defines": [
                "ARDUINO=10812",
                "ARDUINO_MAPLEMINI_F103CB",
                "BOARD_NAME=MAPLEMINI_F103CB",
                "ARDUINO_ARCH_STM32",
                "HAL_UART_MODULE_ENABLED",
                "STM32F103xB",
                "STM32F1xx"
            ]
        },
        {
            "name": "Windows",
