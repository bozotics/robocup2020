                "C:\\Users\\${env:USERNAME}\\AppData\\Local\\Arduino15\\packages\\STM32\\hardware\\stm32\\${Arduino_Core_STM32_version}\\cores\\arduino",
                "C:\\Users\\${env:USERNAME}\\AppData\\Local\\Arduino15\\packages\\STM32\\hardware\\stm32\\${Arduino_Core_STM32_version}\\cores\\arduino\\stm32",
                "C:\\Users\\${env:USERNAME}\\AppData\\Local\\Arduino15\\packages\\STM32\\hardware\\stm32\\${Arduino_Core_STM32_version}\\cores\\arduino\\stm32\\LL",
                "C:\\Users\\${env:USERNAME}\\AppData\\Local\\Arduino15\\packages\\STM32\\hardware\\stm32\\${Arduino_Core_STM32_version}\\variants\\MAPLEMINI_F103CB",
                "C:\\Users\\${env:USERNAME}\\AppData\\Local\\Arduino15\\packages\\STM32\\hardware\\stm32\\${Arduino_Core_STM32_version}\\system\\STM32F1xx",
                "C:\\Users\\${env:USERNAME}\\AppData\\Local\\Arduino15\\packages\\STM32\\hardware\\stm32\\${Arduino_Core_STM32_version}\\system\\Drivers\\STM32F1xx_HAL_Driver\\Src",
